{
  "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": ["./tsconfig.json"]
  },
  "env": {
    "jest": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "camelcase": "off",
    "no-unsafe-optional-chaining": "error",
    "default-param-last": "error"
  },
  "globals": {
    "document": true,
    "FormData": true,
    "navigator": true,
    "window": true
  },
  "root": true
}
